<clientConfig version="1.1">
  <emailProvider id="rebro.net">
    <domain>130k2.rebro.net</domain>
    <domain>grobari.ru</domain>
    <domain>b2.rebro.net</domain>
    <domain>efest.rebro.net</domain>
    <domain>nts.rebro.net</domain>
    <domain>rebro.net</domain>
    <domain>school158.ru</domain>
    <domain>specprofmat.ru</domain>
    <displayName>RebroNet Mail</displayName>
    <displayShortName>RebroNet</displayShortName>
    <incomingServer type="imap">
      <hostname>imap.gmail.com</hostname>
      <port>993</port>
      <socketType>SSL</socketType>
      <username>%EMAILADDRESS%</username>
      <authentication>password-cleartext</authentication>
    </incomingServer>
    <incomingServer type="pop3">
      <hostname>pop.gmail.com</hostname>
      <port>995</port>
      <socketType>SSL</socketType>
      <username>%EMAILADDRESS%</username>
      <authentication>password-cleartext</authentication>
    </incomingServer>
    <outgoingServer type="smtp">
      <hostname>smtp.gmail.com</hostname>
      <port>465</port>
      <socketType>SSL</socketType>
      <username>%EMAILADDRESS%</username>
      <authentication>password-cleartext</authentication>
    </outgoingServer>
    <enable visiturl="https://mail.google.com/mail/u/0/#settings/fwdandpop">
      <instruction>Check 'Enable IMAP' on setting setup page</instruction>
      <instruction lang="ru">Включите настройку imap.gmail.com</instruction>
    </enable>
  </emailProvider>
</clientConfig>
